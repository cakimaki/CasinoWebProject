@model IEnumerable<CasinoWebProject.Models.Item>

<h2>Items</h2>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
                <tr>
                    <td>@item.Name</td>
                    <td>@item.Price</td>
                </tr>
        }
    </tbody>
</table>
